#include <stdlib.h>
#include <time.h>
/*This program assigns
 * a random number to
 *the variable n each time it is
 * executed
*/
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
         if (n % 10 > 5) 
         { 
                 printf("Last digit of,%d,is greater than 5\n", n); 
         } 
         else if (n % 10 == 0) 
         { 
                 printf("Last digit of,%d,is 0\n", n); 
         } 
         else if (n % 10 < 6 && n % 10 > 0) 
         { 
                 printf("Last digit of,%d,is less than 6 and not 0\n", n);
         } 
return (0); 
}
